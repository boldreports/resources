      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select JO.JO_ID as JoID,EP.EMP_NAME,DATE_FORMAT(CURDATE(), '%d/%m/%Y') as 'System_Date',
(select b.JO_BENEFIT_DESC from BE_JOB_ORDER_BENEFIT b join BE_JOB_ORDER_BENEFIT_RELATION a on b.JO_BENEFIT_ID=a.JO_BENEFIT_ID and b.JO_BENEFIT_DESC = 'Medical Insurance' where a.JO_ID = JO.JO_ID) AS Medical,
(select b.JO_BENEFIT_DESC from BE_JOB_ORDER_BENEFIT b join BE_JOB_ORDER_BENEFIT_RELATION a on b.JO_BENEFIT_ID=a.JO_BENEFIT_ID and b.JO_BENEFIT_DESC = 'Food' where a.JO_ID = JO.JO_ID) AS Food,
(select b.JO_BENEFIT_DESC from BE_JOB_ORDER_BENEFIT b join BE_JOB_ORDER_BENEFIT_RELATION a on b.JO_BENEFIT_ID=a.JO_BENEFIT_ID and b.JO_BENEFIT_DESC = 'Accomidation' where a.JO_ID = JO.JO_ID) AS Accomodation,
(select b.JO_BENEFIT_DESC from BE_JOB_ORDER_BENEFIT b join BE_JOB_ORDER_BENEFIT_RELATION a on b.JO_BENEFIT_ID=a.JO_BENEFIT_ID and b.JO_BENEFIT_DESC = 'Transportation' where a.JO_ID = JO.JO_ID) AS Transport,
-- ,JOB1.JO_BENEFIT_DESC,JOB2.JO_BENEFIT_DESC,JOB3.JO_BENEFIT_DESC,JOB4.JO_BENEFIT_DESC,
CONCAT(COALESCE(EA.ADDR_1,''),' ',COALESCE(EA.ADDR_2,''),' ',COALESCE(EA.ADDR_3,''),' ',COALESCE(EA.ADDR_4,''),' ',COALESCE(EA.ADDR_5,'')) AS EMP_ADDRESS,
CONCAT(COALESCE(RA.ADDR_1,''),' ',COALESCE(RA.ADDR_2,''),' ',COALESCE(RA.ADDR_3,''),' ',COALESCE(RA.ADDR_4,''),' ',COALESCE(RA.ADDR_5,'')) AS RA_ADDRESS,
CONCAT(COALESCE(JOWS.ADDR_1,''),' ',COALESCE(JOWS.ADDR_2,''),' ',COALESCE(JOWS.ADDR_3,''),' ',COALESCE(JOWS.ADDR_4,''),' ',COALESCE(JOWS.ADDR_5,'')) AS WORK_SITE_ADDRESS,
EA.ZIPCODE,RC.CITY_DESC, RCON.CNTRY_DESC,RCON.CURRENCY,EP.CONTACT_NO,EP.FAX_NO,TD.DOC_MGT_ID AS 'EMP_LOGO',RD.DOC_DESC,TD.DOC_ID,EO.FIRST_NAME AS 'EMP_PIC_NM',RP.RA_NAME AS 'RA_PIC_NM',
RPOS.POSITION_DESC as 'Agent Position',RP.RA_REG_NO,JOC.CONTRACT_PERIOD, JOC.PROBATION_PERIOD,JOC.SALARY,JOC.OT_AMT_AM,JOC.OT_AMT_PM,JOC.SALARY_TYPE_MTDT_ID,JOC.PYMNT_METHOD,
JOC.PAYDAY,JOC.NO_WORK_HOUR,JOC.NO_WORK_DAY,JOC.ANNUAL_LEAVE,EO.EMP_PROF_ID, TDL.DOC_MGT_ID as SIGN_DOC_MGT_ID , 
 DATE_FORMAT(JOC.EXPECTED_START_DT, '%d/%m/%Y') as 'EXPECTED_START_DT',DATE_FORMAT(JOC.EXPECTED_END_DT,'%d/%m/%Y') as EXPECTED_END_DT,
TDL.DOC_REF_NO as 'SIGN_DOC_REF_NO' , RDL.DOC_DESC as 'SIGN_DOC_DESC',TDL.DOC_ID AS 'SIGN_DOC_ID',JOT.TERMS_CONDITIONS
from BE_JOB_ORDER JO
join BE_EMP_PROFILE EP on JO.EMP_PROF_ID = EP.EMP_PROF_ID
left join BE_TRXN_DOCUMENTS TD on EP.DOC_REF_NO = TD.DOC_REF_NO
join REF_DOCUMENTS RD on TD.DOC_ID = RD.DOC_ID and RD.DOC_DESC ='COMPANY LOGO'
join BE_TRXN_DOCUMENTS TDL on EP.DOC_REF_NO = TDL.DOC_REF_NO
join REF_DOCUMENTS RDL on TDL.DOC_ID = RDL.DOC_ID and RDL.DOC_DESC ='SIGNATURE'
join BE_EMP_ADDRESS EA on EP.EMP_PROF_ID = EA.EMP_PROF_ID
join BE_EMP_OWNER EO on EP.EMP_PROF_ID = EO.EMP_PROF_ID and EO.STATUS = 1
join BE_RA_OWNER RO on JO.RA_PROF_ID = RO.RA_PROF_ID
join BE_RA_PROFILE RP on JO.RA_PROF_ID = RP.RA_PROF_ID and RP.STATUS = 1
join BE_RA_ADDRESS RA on JO.RA_PROF_ID = RA.RA_PROF_ID
join BE_JOB_ORDER_CONTRACT JOC on JO.JO_CNTRCT_ID = JOC.JO_CNTRCT_ID
left join BE_JOB_ORDER_WORK_SITE JOWS on JO.JO_WORK_SITE_ID = JOWS.JO_WORK_SITE_ID
/*join BE_JOB_ORDER_BENEFIT_RELATION JOBR1 on JO.JO_ID = JOBR1.JO_ID
join BE_JOB_ORDER_BENEFIT JOB1 on JOB1.JO_BENEFIT_ID = JOBR1.JO_BENEFIT_ID and JOB1.JO_BENEFIT_DESC = 'Medical Insurance'
join BE_JOB_ORDER_BENEFIT_RELATION JOBR2 on JO.JO_ID = JOBR2.JO_ID
join BE_JOB_ORDER_BENEFIT JOB2 on JOB2.JO_BENEFIT_ID = JOBR2.JO_BENEFIT_ID and JOB2.JO_BENEFIT_DESC = 'Food'
join BE_JOB_ORDER_BENEFIT_RELATION JOBR3 on JO.JO_ID = JOBR3.JO_ID
join BE_JOB_ORDER_BENEFIT JOB3 on JOB3.JO_BENEFIT_ID = JOBR3.JO_BENEFIT_ID and JOB3.JO_BENEFIT_DESC = 'Accomidation'
join BE_JOB_ORDER_BENEFIT_RELATION JOBR4 on JO.JO_ID = JOBR4.JO_ID
join BE_JOB_ORDER_BENEFIT JOB4 on JOB4.JO_BENEFIT_ID = JOBR4.JO_BENEFIT_ID and JOB4.JO_BENEFIT_DESC = 'Transportation'
-- and JOBR.JO_BENEFIT_ID = BE_JOB_ORDER_BENEFIT.JO_BENEFIT_ID*/
left join BE_EMP_OWNER EPW on JO.EMP_PROF_ID = EPW.EMP_PROF_ID and EPW.STATUS = 1
left join REF_POSITION RPOS on RO.POSITION_CD = RPOS.POSITION_CD
left join REF_CITY RC on RC.CITY_CD= EA.CITY_CD
left join REF_COUNTRY RCON on RCON.CNTRY_CD = EA.CNTRY_CD
left join BE_JOB_ORDER_TERMS JOT on JOT.JO_ID = JO.JO_ID where JO.JO_ID =@JoID;</CommandText>
        <QueryParameters>
          <QueryParameter Name="@JoID">
            <Value>=Parameters!JoID.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
