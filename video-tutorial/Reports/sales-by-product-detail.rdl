  <DataSets>
    <DataSet Name="ProductSalesDetails">
      <Query>
        <DataSourceName>AdventuresWorks</DataSourceName>
        <CommandText>SELECT [Sales].[SalesOrderDetail].[ProductID],
	   [AdventureWorks].[Production].[Product].[Name],
	   [AdventureWorks].[Sales].[Store].[Name] AS Shop,
	   SUM([Sales].[SalesOrderDetail].[UnitPrice]) AS Unit,
	   SUM([Sales].[SalesOrderDetail].[OrderQty]) AS Quantity,
	   SUM([Sales].[SalesOrderDetail].[UnitPrice]*[Sales].[SalesOrderDetail].[OrderQty]) AS Sales
FROM [Sales].[SalesOrderDetail]
INNER JOIN [AdventureWorks].[Sales].[SalesOrderHeader] ON [Sales].[SalesOrderDetail].[SalesOrderID] = [AdventureWorks].[Sales].[SalesOrderHeader].[SalesOrderID]
INNER JOIN [AdventureWorks].[Sales].[Store] ON [Sales].[SalesOrderHeader].[CustomerID] = [AdventureWorks].[Sales].[Store].[CustomerID]
INNER JOIN [AdventureWorks].[Production].[Product] ON [Sales].[SalesOrderDetail].ProductID = [AdventureWorks].[Production].[Product].[ProductID]
WHERE [AdventureWorks].[Production].[Product].[Name] = @ProductName
Group By [Sales].[SalesOrderDetail].[ProductID], [AdventureWorks].[Sales].[Store].[Name], [AdventureWorks].[Production].[Product].[Name]</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ProductName">
            <Value>=Parameters!ProductName.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="ProductID">
          <DataField>ProductID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Shop">
          <DataField>Shop</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Unit">
          <DataField>Unit</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Quantity">
          <DataField>Quantity</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Sales">
          <DataField>Sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
